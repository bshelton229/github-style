{
    "manifest_version": 2,
    "name": "Github Style",
    "description": "Github style",
    "version": "0.1",
    "permissions": [
        "https://github.com/"
    ],
    "content_scripts": [
        {
            "matches": ["https://github.com/*"],
            "css": ["css/github-stye.css"]
        }
    ]
}
